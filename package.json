{
	"name": "unplugin-svelte-components",
	"version": "0.1.4",
	"packageManager": "pnpm@7.1.7",
	"description": "Components auto importing for svelte",
	"homepage": "https://github.com/Mohamed-Kaizen/unplugin-svelte-components",
	"bugs": "https://github.com/Mohamed-Kaizen/unplugin-svelte-components/issues",
	"author": "Mohamed Nesredin<https://github.com/Mohamed-Kaizen>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Mohamed-Kaizen/unplugin-svelte-components.git"
	},
	"keywords": [
		"unplugin",
		"vite",
		"webpack",
		"rollup",
		"esbuild",
		"auto-import",
		"svelte",
		"svelte-auto-import"
	],
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs"
		},
		"./vite": {
			"require": "./dist/vite.js",
			"import": "./dist/vite.mjs"
		},
		"./webpack": {
			"require": "./dist/webpack.js",
			"import": "./dist/webpack.mjs"
		},
		"./rollup": {
			"require": "./dist/rollup.js",
			"import": "./dist/rollup.mjs"
		},
		"./esbuild": {
			"require": "./dist/esbuild.js",
			"import": "./dist/esbuild.mjs"
		},
		"./types": {
			"require": "./dist/types.js",
			"import": "./dist/types.mjs"
		},
		"./*": "./*"
	},
	"files": [
		"dist",
		"*.d.ts"
	],
	"scripts": {
		"build": "tsup && pnpm build:fix",
		"dev": "tsup --watch src",
		"dev:link": "pnpm link ./",
		"build:fix": "esno scripts/postbuild.ts",
		"lint": "eslint .",
		"play:kit": "pnpm -C playground/sveltekit run dev",
		"prepublishOnly": "npm run build",
		"release": "bumpp --commit --push --tag",
		"test": "vitest",
		"install_hook": "pnpx simple-git-hooks"
	},
	"dependencies": {
		"@antfu/utils": "^0.5.2",
		"@rollup/pluginutils": "^4.2.1",
		"chokidar": "^3.5.3",
		"fast-glob": "^3.2.11",
		"local-pkg": "^0.4.1",
		"magic-string": "^0.26.2",
		"minimatch": "^5.1.0",
		"resolve": "^1.22.0",
		"unplugin": "^0.7.0"
	},
	"devDependencies": {
		"@types/minimatch": "^3.0.5",
		"@types/node": "^17.0.34",
		"@types/resolve": "^1.20.2",
		"@typescript-eslint/eslint-plugin": "^5.27.1",
		"@typescript-eslint/parser": "^5.27.1",
		"bumpp": "^7.1.1",
		"eslint": "^8.17.0",
		"eslint-config-prettier": "^8.5.0",
		"esno": "^0.16.3",
		"lint-staged": "^13.0.1",
		"pathe": "^0.3.0",
		"prettier": "^2.6.2",
		"rollup": "^2.75.6",
		"simple-git-hooks": "^2.8.0",
		"svelte": "^3.48.0",
		"tsup": "^6.1.2",
		"typescript": "^4.7.3",
		"vite": "^2.9.12",
		"vitest": "^0.14.2"
	},
	"simple-git-hooks": {
		"pre-commit": "npx lint-staged"
	},
	"lint-staged": {
		"*.{js,ts,tsx}": [
			"eslint --fix"
		]
	},
	"engines": {
		"node": ">=14"
	}
}
